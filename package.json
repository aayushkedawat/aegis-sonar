{
  "name": "aegis-sonar",
  "version": "1.0.3",
  "description": "Aegis â€” a Git pre-push shield for SonarQube Quality Gate (JS/TS).",
  "type": "module",
  "bin": {
    "aegis": "bin/cli.js"
  },
  "main": "src/run.js",
  "files": [
    "bin/",
    "src/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
